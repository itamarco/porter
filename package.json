{
  "name": "porter",
  "version": "1.0.0",
  "description": "Kubernetes port-forward manager for macOS",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 && npm run build:electron && NODE_ENV=development electron .",
    "build": "tsc && vite build && npm run prepare:gke-plugin && electron-builder",
    "build:arm64": "tsc && vite build && npm run prepare:gke-plugin && electron-builder --mac --arm64",
    "build:x64": "tsc && vite build && npm run prepare:gke-plugin && electron-builder --mac --x64",
    "build:electron": "tsc -p electron/tsconfig.json",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare:gke-plugin": "node scripts/fetch-gke-plugin.js",
    "generate-icon": "./scripts/generate-icon.sh"
  },
  "keywords": [
    "kubernetes",
    "port-forward",
    "k8s",
    "macos"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^39.2.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tar": "^6.1.11",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "vite": "^7.2.6",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.4.0",
    "electron-log": "^5.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7"
  }
}
